  "main": "index.js",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "cd server && nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run client\" \"npm run server\"",
    "client-build":"cd client && npm ci && npm run build && cd ../",
